{
    "version": 2,
    "builds": [
        {
            "src": "public/**",
            "use": "@vercel/static-build"
        }
    ],
    "routes": [
        {
            "src": "/public/(.*)",
            "dest": "/public/$1"
        },
        {
            "src": "/storage/(.*)",
            "dest": "/storage/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/index.php"
        }
    ]
}